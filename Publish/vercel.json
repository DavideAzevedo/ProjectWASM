{
  "version": 2,
  "headers": [
    {
      "source": "/_framework/(.*\\.wasm)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/_framework/(.*\\.js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/_framework/:path*",
      "destination": "/_framework/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": true
}